DEFVAR hw_text "Hello,world!",0b1010
DEFVAR hw_len 0b1101 ;TODO: 長さを自動で取得する方法をつくる
;DEFVAR_NI var SIZE 10 ;みたいなのも作るか？DEFine VARiable Not Initialized

GLOBAL _start

_start:

;組み込み関数のアドレスにするため、最上位ビットを立てる
PUSHL 1
POP r0
PUSHL 0b111111
POP r1
LSHIFT r0,r1

;組み込み関数writeを呼ぶ
PUSHL 1
POP r0
PUSHL64 ${hw_len}
PUSHL64 ${hw_text}
PUSHL 1
CALL r0

;組み込み関数exitを呼ぶ
PUSHL 0b111100
POP r0
PUSHL 0
CALL r0
